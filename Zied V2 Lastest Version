local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()

local window = DrRayLibrary:Load("Main", "Default")

local tab = DrRayLibrary.newTab("Aiming", "11767037107")

tab.newToggle("Camlock", "Toggle! (Click Once!)", true, function(toggleState)
loadstring(game:HttpGet("https://raw.githubusercontent.com/TrendyScripts/Zied-V3-Scripts/refs/heads/main/Zied%20V2%20Lock"))()
    if toggleState then
        print("On")
    else
        print("Off")
    end
end)

tab.newToggle("Auto Air", "Toggle! (Click Once!)", true, function(toggleState)
loadstring(game:HttpGet("https://raw.githubusercontent.com/TrendyScripts/Zied-V3-Scripts/refs/heads/main/Zied%20V2%20Auto-Air"))()
    if toggleState then
        print("On")
    else
        print("Off")
    end
end)

